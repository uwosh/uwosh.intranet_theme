/*=========================
       SEARCH
==========================*/

.LSBox .searchButton {
background-image: linear-gradient(bottom, rgb(80,80,88) 22%, rgb(91,91,101) 61%, rgb(36,36,39) 81%);
background-image: -o-linear-gradient(bottom, rgb(80,80,88) 22%, rgb(91,91,101) 61%, rgb(36,36,39) 81%);
background-image: -moz-linear-gradient(bottom, rgb(80,80,88) 22%, rgb(91,91,101) 61%, rgb(36,36,39) 81%);
background-image: -webkit-linear-gradient(bottom, rgb(80,80,88) 22%, rgb(91,91,101) 61%, rgb(36,36,39) 81%);
background-image: -ms-linear-gradient(bottom, rgb(80,80,88) 22%, rgb(91,91,101) 61%, rgb(36,36,39) 81%);
-webkit-border-radius: 10px;
-moz-border-radius: 10px;
border-radius: 10px;
color:white;
padding:3px 20px;
border-style:none;

}

#LSResult {
z-index:30;
}

/*=========================
       PERSONAL TOOLBAR
==========================*/
#portal-personaltools-wrapper {
position:absolute;
top:0px;
z-index:50;
}

/*=========================
       TABS
==========================*/
#portal-globalnav {
font-family:"Futura";
font-stretch:condensed;
font-size:1em;
}

#portal-globalnav, #portal-globalnav li a{
background: rgb(97,104,112); /* Old browsers */
background: -moz-linear-gradient(top, rgba(97,104,112,1) 0%, rgba(43,49,54,1) 100%); /* FF3.6+ */
background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(97,104,112,1)), color-stop(100%,rgba(43,49,54,1))); /* Chrome,Safari4+ */
background: -webkit-linear-gradient(top, rgba(97,104,112,1) 0%,rgba(43,49,54,1) 100%); /* Chrome10+,Safari5.1+ */
background: -o-linear-gradient(top, rgba(97,104,112,1) 0%,rgba(43,49,54,1) 100%); /* Opera 11.10+ */
background: -ms-linear-gradient(top, rgba(97,104,112,1) 0%,rgba(43,49,54,1) 100%); /* IE10+ */
background: linear-gradient(top, rgba(97,104,112,1) 0%,rgba(43,49,54,1) 100%); /* W3C */
filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#616870', endColorstr='#2b3136',GradientType=0 ); /* IE6-9 */
color:white;
}

#portal-globalnav .selected a {
background-color:#7f8891;
}

#portal-globalnav li a{
padding-bottom:10px!important;
border:none;
margin:0px;
min-width:3em;
}


#portal-globalnav ul.submenu li a{
background-color:#2b3136!important;
color:white!important;
background-image:none!important;
padding-left:20px!important;
font-stretch:normal;
font-size:.8em;

}

#portal-globalnav li#portaltab-for_faculty a {
background-image:URL(arrowTrans.png);
background-repeat:no-repeat;
background-position:left;
padding-left:30px;
}

#portaltab-for_faculty.plain, #portaltab-for_staff .plain, #portaltab-for_students .plain, #portaltab-Groups .plain {
background: rgb(138,138,138); /* Old browsers */
background: -moz-linear-gradient(top, rgba(138,138,138,1) 0%, rgba(96,96,96,1) 100%); /* FF3.6+ */
background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(138,138,138,1)), color-stop(100%,rgba(96,96,96,1))); /* Chrome,Safari4+ */
background: -webkit-linear-gradient(top, rgba(138,138,138,1) 0%,rgba(96,96,96,1) 100%); /* Chrome10+,Safari5.1+ */
background: -o-linear-gradient(top, rgba(138,138,138,1) 0%,rgba(96,96,96,1) 100%); /* Opera 11.10+ */
background: -ms-linear-gradient(top, rgba(138,138,138,1) 0%,rgba(96,96,96,1) 100%); /* IE10+ */
background: linear-gradient(top, rgba(138,138,138,1) 0%,rgba(96,96,96,1) 100%); /* W3C */
filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#8a8a8a', endColorstr='#606060',GradientType=0 ); /* IE6-9 */
}

#portaltab-RESOURCES_FOR_LABEL.plain a{
background-image:URL(dots.png);
background-repeat:no-repeat;
background-position:3px 

}


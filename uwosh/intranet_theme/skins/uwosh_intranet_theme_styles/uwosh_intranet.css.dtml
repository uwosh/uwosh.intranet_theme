 /*========================================
   TOP LEVEL NAVIGATION
 ==========================================*/

 #top_level_navigation {
	font-family: Helvetica;
	font-size: 11px;
	line-height: 17px;
 	display:inline;
 	width:100%;
 	float:left;
    text-align:center;
	background-image:url(&dtml-portal_url;/background.jpg);
	background-attachment:fixed;
	background-position:top center;

 }

  #top_level_navigation ul {
	display:inline-block;
/*float:left;*/
 	text-align:center;
 	width:100%;
 	padding-top:5px;
 	margin:0px;
	width:880px;
 	background-image:url(&dtml-portal_url;/siteActionsBak.jpg);
 	background-repeat:repeat-x;
 	background-position:top;
        background-color:#717173;
 }


 #top_level_navigation ul li {
 	float:left;
 	list-style-type:none;
 }

 #top_level_navigation ul li a {
 	padding:2px 10px 2px 10px;
 	display:block;
 	border-right:solid 1px #999999;
 	color:#ffffff;
 	border-bottom:none;
 	text-decoration:none;
 }

 #top_level_navigation ul li{
 	float:left;
 	list-style-type:none;
 }

 #portal-top .titan_servicesNav {
 	float:right;
	margin-right:10px;
 	background-image:url(&dtml-portal_url;/titanServicesBak.jpg);
 }

 #portal-top .titan_servicesNav a {
 	background-image:url(&dtml-portal_url;/arrow.png);
 	background-repeat:no-repeat;
 	background-position: right center;
 	padding-right:20px;
	border-right:none;
 }


/*========================================
   FOOTER
 ==========================================*/

#uwoshFooter {
	font-family: Helvetica;
	font-size: 11px;
	line-height: 17px;
	width: 78px;
	background-image:url(&dtml-portal_url;/footerBak.gif);
	background-position:top;
	background-repeat:no-repeat;
	background-color:white!important;
	float:left;
	width:940px;
	padding-top:50px;
	padding-bottom:20px;
}
ul#footerMap:first-child {
	margin-left:60px;
}
.footerMap {
	float:left;
	width:130px;
	margin:0px;
}
.footerMap li {
	list-style-type:none;
	margin:1px;
}
.footerMap li a {
	text-decoration:none;
	color:#666666;
}
#resources table {
	text-align:center;
	width:300px;
	margin-right:10px;
	float:right;
}
#resources img {
	padding-bottom:5px;
}
.footerMap .small-textMap, .footerMap .normal-textMap, .footerMap .large-textMap {
	display:none;
}


